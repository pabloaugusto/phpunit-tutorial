{
    "name": "pabloaugusto/phpunit-tutorial",
    "description": "A simple PHP project to learn phpunit TDD",
    "type": "project",
    "version" : "1.0.0",
    "license": "MIT",
    "homepage": "https://github.com/pabloaugusto/phpunit-tutorial",
    "keywords": [
        "phpunit", "php", "tdd", "test", "code coverage", "tutorial", "dummies"
    ],
    "authors": [
        {
            "name": "Pablo Augusto",
            "email": "pablo@pabloaugusto.com",
            "homepage": "https://github.com/pabloaugusto",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "satooshi/php-coveralls": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
